		"devdoc": {
			"details": "This contract allows users to create, manage, and interact with their personal shopping lists.",
			"kind": "dev",
			"methods": {
				"addItem(string,string,uint256)": {
					"details": "Adds an item with a specified name and quantity to a specific list created by the sender.",
					"params": {
						"itemName": "The name of the item to add.",
						"listName": "The name of the list to add the item to.",
						"quantity": "The quantity of the item to add."
					}
				},
				"createList(string)": {
					"details": "Creates a new shopping list for the sender with the specified name.",
					"params": {
						"name": "The name of the list to create."
					}
				},
				"getItemNames(string)": {
					"details": "Retrieves the names of all items in a specific list created by the sender.",
					"params": {
						"listName": "The name of the list to retrieve item names from."
					},
					"returns": {
						"_0": "An array of strings containing the names of the items in the list."
					}
				},
				"getListNames()": {
					"details": "Retrieves the names of all lists created by the sender.",
					"returns": {
						"_0": "An array of strings containing the names of the lists."
					}
				}
